{
  "manifest_version": 2,
  "name": "Notion Reading List",
  "version": "1.0.0",
  "description": "A browser extension to capture URLs and manage reading list",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "https://api.notion.com/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Notion Reading List"
  },
  "icons": {
    "16": "icons/logo-16.png",
    "48": "icons/logo-48.png",
    "128": "icons/logo-128.png"
  }
}